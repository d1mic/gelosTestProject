{
  "name": "project",
  "version": "1.0.0",
  "description": "Gellos project",
  "main": "index.js",
  "scripts": {
    "client:dev": "REACT_APP_MOCK_DATA=true npm start --workspace=client",
    "client": "npm start --workspace=client",
    "imdb": "npm start --workspace=imdb-service",
    "books": "npm start --workspace=books-service",
    "test:client": "npm test --workspace=client",
    "test:imdb": "npm test --workspace=imdb-service",
    "test:books": "npm test --workspace=books-service",
    "test:integration": "npm run test:integration --workspace=client",
    "test:e2e": "npm run test:e2e --workspace=client",
    "test:e2e:headed": "npm run test:e2e:headed --workspace=client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "database:init": "node scripts/initDatabase.js",
    "database:init:imdb": "npm run database:init -- --type=imdb --out=db-imdb.sqlite",
    "database:init:books": "npm run database:init -- --type=books --out=db-books.sqlite",
    "database:import:imdb": "bash scripts/dataimportMovies.sh",
    "database:import:books": "bash scripts/dataimportBooks.sh",
    "report": "npm run report --workspace=client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d1mic/gelosTestProject.git"
  },
  "author": "Nikola Dimic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d1mic/gelosTestProject/issues"
  },
  "homepage": "https://github.com/d1mic/gelosTestProject#readme",
  "dependencies": {
    "minimist": "^1.2.6"
  },
  "workspaces": [
    "imdb-service",
    "books-service",
    "client"
  ],
  "type": "module"
}
